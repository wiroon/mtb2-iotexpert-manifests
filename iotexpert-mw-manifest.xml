<middleware>

  <middleware>
    <name>ntshell</name>
    <id>ntshell</id>
    <uri>https://github.com/iotexpert/middleware-ntshell</uri>
    <desc>NT Shell library for Cypress PSoC6 Anycloud</desc>
    <category>IoT Expert</category>
    <req_capabilities>psoc6</req_capabilities>
    <versions>
      <version flow_version="1.0">
        <num>Latest 1.X release</num>
        <commit>latest-v1.X</commit>
        <desc>Latest 1.X release</desc>
      </version>
      <version flow_version="1.0,2.0">
        <num>Latest 2.X release</num>
        <commit>latest-v2.X</commit>
        <desc>Latest 2.X release</desc>
      </version>
    </versions>
  </middleware>

  <middleware>
      <name>Bluetooth Utilities</name>
    <id>btutil</id>
    <uri>https://github.com/iotexpert/btutil</uri>
    <desc>A library of Bluetooth Debugging Utilties for Cypress PSoC6 Anycloud</desc>
    <category>IoT Expert</category>
    <req_capabilities>psoc6</req_capabilities>
    <versions>
      <version flow_version="1.0,2.0">
        <num>master</num>
        <commit>master</commit>
        <desc>master</desc>
      </version>
    </versions>
  </middleware>
  
  <middleware>  
    <name>IoT Expert Graphics</name>
    <id>iotexpert-graphics</id>
    <uri>https://github.com/iotexpert/IoTExpertGraphics</uri>
    <desc>A library of IoT Expert Graphics</desc>
    <category>IoT Expert</category>
    <req_capabilities>psoc6</req_capabilities>
    <versions>
      <version flow_version="1.0,2.0">
        <num>master</num>
        <commit>master</commit>
        <desc>master</desc>
      </version>
    </versions>
  </middleware>

